def find_largest_smallest(numbers):
    largest = numbers[0]
    smallest = numbers[0]
    for num in numbers:
        if num > largest:
            largest = num
        if num < smallest:
            smallest = num
    return largest, smallest

# Example usage
my_list = [4, 7, 1, 9, 3]
largest, smallest = find_largest_smallest(my_list)
print("Largest:", largest)
print("Smallest:", smallest)
